  <owl:NamedIndividual rdf:about="http://example.org/career#Doctor">
    <rdf:type rdf:resource="http://example.org/career#Career"/>
    <career:requires rdf:resource="http://example.org/career#PatientCare"/>
    <career:requires rdf:resource="http://example.org/career#Anatomy"/>
    <career:requires rdf:resource="http://example.org/career#MedicalResearch"/>
    <career:requires rdf:resource="http://example.org/career#CPR"/>
