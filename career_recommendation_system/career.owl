    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#Networking">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#ProblemSolving">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#Programming">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#MechanicalDesign">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
