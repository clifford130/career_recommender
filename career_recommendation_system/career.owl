  <owl:NamedIndividual rdf:about="http://example.org/career#C_Cplusplus">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#Architecture">
