  </owl:NamedIndividual>

  <!-- Define Careers as Individuals -->
  <owl:NamedIndividual rdf:about="http://example.org/career#SoftwareDeveloper">
    <rdf:type rdf:resource="http://example.org/career#Career"/>
    <career:requires rdf:resource="http://example.org/career#Java"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#DataScientist">
    <rdf:type rdf:resource="http://example.org/career#Career"/>
    <career:requires rdf:resource="http://example.org/career#Python"/>
    <career:requires rdf:resource="http://example.org/career#DataAnalysis"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#UXDesigner">
