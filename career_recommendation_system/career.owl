  <owl:NamedIndividual rdf:about="http://example.org/career#ETL">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>

  <!-- Skills for Medicine -->
  <owl:NamedIndividual rdf:about="http://example.org/career#PatientCare">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#Anatomy">
