<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/career#"
         xml:base="http://example.org/career"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:career="http://example.org/career#">

  <!-- Ontology Declaration -->
  <owl:Ontology rdf:about="http://example.org/career"/>

  <!-- Define Classes -->
  <owl:Class rdf:about="http://example.org/career#Career"/>
  <owl:Class rdf:about="http://example.org/career#Skill"/>

  <!-- Define the 'requires' property using the career prefix -->
  <owl:ObjectProperty rdf:about="http://example.org/career#requires">
    <rdfs:domain rdf:resource="http://example.org/career#Career"/>
    <rdfs:range rdf:resource="http://example.org/career#Skill"/>
  </owl:ObjectProperty>

  <!-- Define Skills as Individuals -->
  <owl:NamedIndividual rdf:about="http://example.org/career#Python">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#DataAnalysis">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#Java">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#UserExperience">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#Cybersecurity">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#DigitalMarketing">
    <rdf:type rdf:resource="http://example.org/career#Skill"/>
  </owl:NamedIndividual>
  <owl:NamedIndividual rdf:about="http://example.org/career#SQL">
